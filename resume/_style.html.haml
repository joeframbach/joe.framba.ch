:css
  body * {
    color: #303030;
  }
  b, em {
    color: #000000;
    font-style: inherit;
    font-weight: inherit;
  }
  .page-header {
    width: 100%;
    margin: 0 auto;
    text-align: center;
  }
  .header-name {
    font-weight: bold;
  }
  .header-address {
    font-size: 0.9rem;
    font-style: italic;
  }
  .section-header {
    width: 100%;
    margin: 0 auto;
    text-align: center;
    margin: 1rem 0 0.25rem 0;
    font-weight: bold;
  }
  .content-header .left {
    font-weight: bold;
    float: left;
  }
  .right {
    float: right;
    font-size: 0.9rem;
  }
  .content-subtext {
    font-size: 0.9rem;
    clear: both;
  }
  ul {
    list-style: none inside;
    margin: 0.5rem auto 0.5rem 1rem;
    padding-left: 1rem;
    text-indent: -1rem;
  }
  ul li:before {
    content: "\203A";
  }
  .top-right-links {
    display: none;
  }
  .top-right-links a {
    padding: 4px;
  }
  @media screen and (min-width: 746px) {
    .page {
      margin: 24px auto;
      padding: 24px;
      width: 684px;
      min-height: 884px;
      border-top: 1px solid black;
      border-right: 3px solid black;
      border-bottom: 3px solid black;
      border-left: 1px solid black;
      position: relative;
    }
    .top-right-links {
      display: block;
      position: absolute;
      top: 10px;
      right: 10px;
    }
  }
  @media screen and (max-width: 746px) {
    .page + .page .page-header {
      display: none;
    }
    .right {
      clear: left;
      float: left;
    }
  }

  @media print {
    @page {
      margin: 0.5cm;
    }
    .page + .page {
      page-break-before: always;
    }
    li a {
      display: none;
    }
  }
